{
  "verbose": true,
  "collectCoverage": true,
  "collectCoverageFrom": [
    "src/reducers/*.{js,jsx}",
    "!<rootDir>/node_modules/",
    "!<rootDir>/src/api/*",
    "!<rootDir>/src/store/*",
    "!<rootDir>/src/tests/**/*",
    "!src/Index.jsx"
  ],
  "coverageReporters": ["json", "lcov", "text"]
}
